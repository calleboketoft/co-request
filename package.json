{
  "name": "co-request",
  "version": "0.0.1",
  "description": "Angular 2 request abstractor",
  "main": "src/co-request/co-request.js",
  "scripts": {
    "start": "node server",
    "build": "npm run tsc",
    "prepublish": "npm run build",
    "tsc": "tsc -p src",
    "watch": "tsc -p src -w",
    "postinstall": "jspm install"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run build"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calleboketoft/co-request.git"
  },
  "author": "Carl-Ola Boketoft",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/calleboketoft/co-request/issues"
  },
  "homepage": "https://github.com/calleboketoft/co-request#readme",
  "devDependencies": {
    "angular2": "^2.0.0-beta.0",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.13",
    "express": "^4.13.3",
    "ghooks": "^1.0.1",
    "jspm": "^0.16.15",
    "reflect-metadata": "^0.1.2",
    "rxjs": "^5.0.0-beta.0",
    "typescript": "^1.7.5",
    "zone.js": "^0.5.10"
  },
  "jspm": {
    "configFile": "jspm.config.js",
    "dependencies": {
      "angular2": "npm:angular2@^2.0.0-beta.0",
      "reflect-metadata": "npm:reflect-metadata@^0.1.2"
    },
    "devDependencies": {
      "typescript": "npm:typescript@^1.7.5"
    }
  }
}
